szam = 0 
while szam != 100:
    szam = szam + 1
    print(szam)
    print("A program vége!")  
    # 1..100

    szam = 0 
while szam != 100:
    print(szam)
    szam = szam + 1
    print("A program vége!")



szam = 1
while true:
    print(szam)
    szam = szam + 1
    print("A program vége!")
    # a programozas futását"CRLT"+"C"-vel tudjuk

szam = 0 
while szam < 100:
    szam = szam +100
    print(szam)
